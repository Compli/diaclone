{
    "name": "picr/diaclone",
    "description": "Transform your data",
    "keywords": [
        "transformation"
    ],
    "license": "MIT",
    "require": {
        "php": ">=7.1.0",
        "nesbot/carbon": "^1.21"
    },
    "require-dev": {
        "codeception/codeception": "^2.2",
        "marc-mabe/php-enum": "^3.0",
        "ramsey/uuid": "^3.0@dev"
    },
    "suggest": {
        "marc-mabe/php-enum": "For enum classes",
        "ramsey/uuid": "For UUID transformation",
        "chefkoch/morphoji": "For emoji transformation"
    },
    "autoload": {
        "psr-4": {
            "Diaclone\\": "src/",
            "Test\\Functional\\": "tests/functional",
            "Test\\Unit\\": "tests/unit"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
